{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "myIpAddress": {
        "value": "${MY_IP_ADDRESS}"
      },
      "myPrincipalId": {
        "value": "${MY_USER_ID}"
      },
      "deployFrontDoor": {
        "value": "${DEPLOY_FRONTDOOR}"
      },
      "deployAse": {
        "value": "${DEPLOY_ASE}"
      },
      "deployLogicApps": {
        "value": "${DEPLOY_LOGIC_APPS}"
      },
      "deployFunctions": {
        "value": "${DEPLOY_FUNCTIONS}"
      },
      "deployServiceBus": {
        "value": "${DEPLOY_SERVICEBUS}"
      },
      "deployApimDevPortal": {
        "value": "${DEPLOY_APIM_DEV_PORTAL}"
      },
      "deployApiCenter": {
        "value": "${DEPLOY_API_CENTER}"
      },
      "apiCenterPortalClientId": {
        "value": "${API_CENTER_PORTAL_CLIENT_ID}"
      },
      "wafRateLimitThreshold": {
        "value": "${WAF_RATE_LIMIT_THRESHOLD}"
      }
    }
}
